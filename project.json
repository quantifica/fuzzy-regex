{
  "name": "tre",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/tre",
  "projectType": "library",
  "tags": [],
  "targets": {
    "autogen": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/tre/vendor/tre",
        "commands": ["./utils/autogen.sh"]
      }
    },
    "configure": {
      "dependsOn": ["autogen"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/tre/vendor/tre",
        "commands": ["./configure"]
      }
    },
    "make": {
      "dependsOn": ["configure"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/tre/vendor/tre",
        "commands": ["make"]
      }
    },
    "build": {
      "dependsOn": ["make"],
      "executor": "nx:run-commands",
      "options": {
        "commands": ["node-gyp --directory libs/tre rebuild"]
      }
    }
  }
}
